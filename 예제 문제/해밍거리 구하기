#include <iostream>
#include <string>

using namespace std;

int main()
{
	string dna1, dna2;
	int cnt = 0;

	cout << "DNA1: ";
	cin >> dna1;
	
	cout << "DNA2: ";
	cin >> dna2;

	for (int i = 0; i < dna1.length(); i++)
	{
		if (dna1[i] == dna2[i])
			cnt++;
	}

	cout << "해밍 거리는 " << cnt << "\n";

  return 0;
}
